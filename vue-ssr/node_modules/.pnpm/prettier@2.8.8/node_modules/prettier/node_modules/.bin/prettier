#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/mnt/d/BaiduNetdiskDownload/day09-SSR服务端渲染配套资料/04-代码/04-代码/02-vue-ssr/node_modules/.pnpm/prettier@2.8.8/node_modules/prettier/node_modules:/mnt/d/BaiduNetdiskDownload/day09-SSR服务端渲染配套资料/04-代码/04-代码/02-vue-ssr/node_modules/.pnpm/prettier@2.8.8/node_modules:/mnt/d/BaiduNetdiskDownload/day09-SSR服务端渲染配套资料/04-代码/04-代码/02-vue-ssr/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/mnt/d/BaiduNetdiskDownload/day09-SSR服务端渲染配套资料/04-代码/04-代码/02-vue-ssr/node_modules/.pnpm/prettier@2.8.8/node_modules/prettier/node_modules:/mnt/d/BaiduNetdiskDownload/day09-SSR服务端渲染配套资料/04-代码/04-代码/02-vue-ssr/node_modules/.pnpm/prettier@2.8.8/node_modules:/mnt/d/BaiduNetdiskDownload/day09-SSR服务端渲染配套资料/04-代码/04-代码/02-vue-ssr/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../bin-prettier.js" "$@"
else
  exec node  "$basedir/../../bin-prettier.js" "$@"
fi
